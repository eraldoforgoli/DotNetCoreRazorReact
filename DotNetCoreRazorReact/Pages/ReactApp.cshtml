@page

@{
    Layout = null;
    ViewData["Title"] = "React App";
}

@* Entry point for our react app *@
@* The id is the same as the one that react references in index.tsx *@
<div id="react-app"></div>

<environment names="Development">
    <script src="~/dist/react_app.js"></script>
</environment>
<environment names="Staging,Production">
    <script src="~/dist/react_app.js" asp-append-version="true"></script>
</environment>
