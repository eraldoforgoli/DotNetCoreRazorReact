{
  "compilerOptions": {
    "outDir": "../wwwroot/dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "lib": [
      "es2016",
      "dom"
    ],
    "baseUrl": "src"
  },
  "include": [
    "src"
  ]
}
